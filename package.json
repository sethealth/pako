{
  "name": "@sethealth/pako",
  "description": "zlib port to javascript - fast, modularized, with browser support",
  "version": "2.0.2",
  "keywords": [
    "zlib",
    "deflate",
    "inflate",
    "gzip"
  ],
  "contributors": [
    "Andrei Tuputcyn (https://github.com/andr83)",
    "Vitaly Puzrin (https://github.com/puzrin)",
    "Friedel Ziegelmayer (https://github.com/dignifiedquire)",
    "Kirill Efimov (https://github.com/Kirill89)",
    "Jean-loup Gailly",
    "Mark Adler"
  ],
  "files": [
    "dist/"
  ],
  "module": "dist/pako.esm.js",
  "main": "dist/pako.js",
  "license": "(MIT AND Zlib)",
  "repository": "nodeca/pako",
  "module": "./dist/pako.esm.mjs",
  "exports": {
    "import": "./dist/pako.esm.mjs",
    "require": "./index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && mocha",
    "coverage": "npm run lint && nyc mocha && nyc report --reporter html",
    "build": "rollup -c",
    "doc": "node support/build_doc.js",
    "gh-doc": "npm run doc && gh-pages -d doc -f",
    "prepublishOnly": "npm run gh-doc"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^10.0.0",
    "eslint": "^7.13.0",
    "gh-pages": "^3.1.0",
    "mocha": "^8.2.1",
    "multiparty": "^4.1.3",
    "ndoc": "^6.0.0",
    "nyc": "^15.1.0",
    "rollup": "^2.33.1",
    "shelljs": "^0.8.4"
  },
  "dependencies": {}
}
